  <h1>Contact</h1>
  <%= form_for @contact do |f| %>

    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <div class="form-group">
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>

    <div class="form-group">
      <%= f.label :message %>
      <%= f.text_area :message %>
    </div>

    <div class="form-actions">
      <%= f.submit %>
    </div>
  <% end %>